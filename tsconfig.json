{
  "compilerOptions": {
    "sourceMap":       true,
    "target":         "es5",
    "module":         "commonjs",
    "outDir":         "./dist",
    "noImplicitAny":   true,
    "strict":          true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
    "noUnusedParameters": false
  }
}
